<div class="box">
    <h2>Browse Users</h2>
    <table>
        {{> browse_header button=true}}
        <tbody>
            {{#each data}}
            <tr row-id={{this.userID}}>
                <td>{{#if this.userID}}{{this.userID}}{{else}}NULL{{/if}}</td>
                <td>{{#if this.username}}{{this.username}}{{else}}NULL{{/if}}</td>
                <td>{{#if this.password}}{{this.password}}{{else}}NULL{{/if}}</td>
                <td><button onclick="deleteUser({{this.userID}})">Delete</button></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <script src="/js/delete_user.js" type="text/js" defer></script>
</div>

<div class="box">
    <h2>Insert User</h2>
    <form action="" method="get" class="form-example">
        <div class="form-example"><label for="name">Username: </label><input type="text" name="name" id="name" required></div>
        <div class="form-example"><label for="password">Password: </label><input type="text" name="password" id="name" required></div>
        <input type="submit">
    </form>
</div>
